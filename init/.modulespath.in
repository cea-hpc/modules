@VERSIONING@@BASEPREFIX@/$MODULE_VERSION/modulefiles	# Module pkg modulefiles (if versioning)
@NOTVERSIONING@@datarootdir@/modulefiles	# Module pkg modulefiles (if no versioning)
@MODULEPATH@				# General module files
