lappend ans [list set LOADEDMODULES extdfl/1.3.7:extdfl/1.3.1]
testouterr_cmd_re sh {load extdfl@1.2.3:1.3.7} $ans {}
testouterr_cmd_re sh {load extdfl@:1.4.5} $ans {}
set ans [list]
lappend ans [list set __MODULES_LMALTNAME extdfl/2.0.1&as|extdfl/latest]
lappend ans [list set _LMFILES_ $mp/extdfl/1.3.7:$mp/extdfl/2.0.1]
lappend ans [list set LOADEDMODULES extdfl/1.3.7:extdfl/2.0.1]
