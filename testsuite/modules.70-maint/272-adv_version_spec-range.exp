setenv_loaded_module [list $mp/extdfl3/1.3/4] [list $mp/extdfl3/1.3/4]
testouterr_cmd sh {unload extdfl3@1.1:} $ans {}
testouterr_cmd sh {unload extdfl3@1.3/3:} ERR $err_specvers'1.3/3:'
