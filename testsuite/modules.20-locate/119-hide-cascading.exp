lappend ans [list set __MODULES_LMALTNAME hide4/.1.0&as|hide4/default&as|hide4/latest]
lappend ans [list set _LMFILES_ $mp/hide4/.1.0]
lappend ans [list set LOADEDMODULES hide4/.1.0]
