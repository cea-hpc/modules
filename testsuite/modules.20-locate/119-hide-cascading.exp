setenv_var TESTSUITE_HIDE1_CASC7_SET2 1
testouterr_cmd sh {load hide1@5:} ERR $err_path'hide1@5:'
testouterr_cmd sh {load hide1@5.0} ERR [err_unablelocate hide1@5.0]
testouterr_cmd sh {avail -t hide1@5:} OK {}
