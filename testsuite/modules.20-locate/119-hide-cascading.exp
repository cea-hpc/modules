setenv_var TESTSUITE_HIDE1_CASC4_SET6 1
testouterr_cmd sh {load hide1@5.0} $ans {}
testouterr_cmd sh {avail -t hide1@5.0} OK "$mp:\nhide1/5.0 <H>"
